debug = true 
appPrefix = /apps/subutai/current/
dataPrefix = /var/lib/apps/subutai/current/
lxcPrefix = /mnt/lib/lxc/    

[management]
gpgUser =
port = 8443
host = 10.10.10.1
login = admin
